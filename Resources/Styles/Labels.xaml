<?xml version="1.0" encoding="UTF-8" ?>
<ResourceDictionary
        xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
        xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
        x:Name="Labels">
    
    <Style TargetType="Label">
        <Setter Property="FontFamily" Value="OpenSansRegular"/>
    </Style>

    <Style TargetType="Label" Class="IconLabel">
        <Setter Property="FontFamily" Value="IcoMoon" />
    </Style>

    <Style TargetType="Label" Class="BlackLabel">
        <Setter Property="BackgroundColor" Value="{StaticResource Black}" />
    </Style>

    <Style TargetType="Label" Class="BoldLabel">
        <Setter Property="FontFamily" Value="IcoMoon" />
    </Style>

</ResourceDictionary>